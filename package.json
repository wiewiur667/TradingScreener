{
  "name": "@project/server",
  "version": "1.0.0",
  "description": "",
  "private":true,
  "scripts": {
    "build": "yarn tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn test:lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn test:unit",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write"
  },
  "dependencies": {
    "@tsed/ajv": "6.8.1",
    "@tsed/common": "6.8.1",
    "@tsed/core": "6.8.1",
    "@tsed/di": "6.8.1",
    "@tsed/exceptions": "6.8.1",
    "@tsed/json-mapper": "6.8.1",
    "@tsed/platform-express": "6.8.1",
    "@tsed/schema": "6.8.1",
    "@tsed/swagger": "6.8.1",
    "@typegoose/typegoose": "^7.4.2",
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.22",
    "@types/mongoose": "^5.10.0",
    "ajv": "6.12.6",
    "axios": "^0.21.0",
    "body-parser": "1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "express": "4.17.1",
    "helmet": "4.2.0",
    "method-override": "3.0.0",
    "mongoose": "^5.10.14"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "2.1.1",
    "@tsed/cli-plugin-jest": "2.1.1",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.8",
    "@types/express": "4.17.9",
    "@types/helmet": "4.0.0",
    "@types/jest": "26.0.15",
    "@types/method-override": "0.0.31",
    "@types/node": "14.14.7",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "concurrently": "5.3.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.1",
    "nodemon": "2.0.6",
    "prettier": "2.1.2",
    "supertest": "6.0.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  }
}
