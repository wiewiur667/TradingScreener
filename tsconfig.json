{
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./buildInfo",
    "module": "commonjs",
    "target": "es2016",
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "isolatedModules": false,
    "suppressImplicitAnyIndexErrors": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "newLine": "LF",
    "noEmit": true,
    "resolveJsonModule": true,
    "lib": [
      "es7",
      "dom",
      "esnext.asynciterable"
    ],
    "typeRoots": [
      "./node_modules/@types"
    ],
    // "baseUrl": ".",
    // "paths": {
    //   "@infrastructure/*":["./src/domain/infrastructure/*"],
    //   "@models/*":["./src/domain/models/*"],
    //   "@plugins/*":["./src/domain/plugins/*"],
    //   "@services/*":["./src/domain/services/*"],
    //   "@stores/*":["./src/domain/stores/*"],
    // }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "./public",
    "dist",
    "test"
  ],
  "linterOptions": {
    "exclude": []
  }
}
